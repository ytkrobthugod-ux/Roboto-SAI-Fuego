{
  "services": {
    "roboto-sai-dev": {
      "volumes": [
        "./:/app:cached",
        "/app/node_modules"
      ],
      "command": "npm run dev"
    },
    "roboto-sai-backend": {
      "volumes": [
        "./backend:/app/backend:cached",
        "./BackendAdds:/app/BackendAdds:cached",
        "./sdk/roboto-sai-sdk/roboto-sai-sdk:/app/sdk/roboto-sai-sdk:cached"
      ],
      "command": "uvicorn main:app --host 0.0.0.0 --port 5000 --reload",
      "ports": ["5000:5000"]
    }
  }
}