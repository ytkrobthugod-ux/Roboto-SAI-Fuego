{
    "name": "roboto-sai-backend",
    "type": "web_service",
    "ownerId": "tea-d5n6ukfgi27c73fhllo0",
    "repo": "https://github.com/Roboto-SAI-LLC/Roboto-SAI-Fuego.git",
    "autoDeploy": "yes",
    "branch": "main",
    "serviceDetails": {
        "env": "docker",
        "dockerfilePath": "./backend/Dockerfile",
        "plan": "starter",
        "healthCheckPath": "/api/health",
        "envVars": [
            {
                "key": "FRONTEND_ORIGIN",
                "value": "https://roboto-sai.onrender.com"
            },
            {
                "key": "FRONTEND_HASH_ROUTER",
                "value": "true"
            },
            {
                "key": "SESSION_TTL_SECONDS",
                "value": "2592000"
            },
            {
                "key": "PYTHON_ENV",
                "value": "production"
            },
            {
                "key": "ROBO_EMOTION_STATE_PATH",
                "value": "/var/data/emotion_state.json"
            }
        ]
    }
}